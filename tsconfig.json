{
    "version": "1.6.0",
    "compileOnSave": false,
    "compilerOptions": {
        "target": "es6",
        "module": "amd",
        "declaration": true,
        "moduleResolution": "node"
    },
    "formatCodeOptions": {
        "indentSize": 4,
        "tabSize": 4,
        "newLineCharacter": "\r\n",
        "convertTabsToSpaces": true,
        "insertSpaceAfterCommaDelimiter": true,
        "insertSpaceAfterSemicolonInForStatements": true,
        "insertSpaceBeforeAndAfterBinaryOperators": true,
        "insertSpaceAfterKeywordsInControlFlowStatements": true,
        "insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
        "insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": false,
        "placeOpenBraceOnNewLineForFunctions": false,
        "placeOpenBraceOnNewLineForControlBlocks": false
    },
    "filesGlob": [
        "./**/*.ts",
        "!./example/**/*.*",
        "!./package/**/*.*",
        "!./node_modules/**/*.*",
        "./node_modules/bobril/**/*.*",
        "./node_modules/fun-model/**/*.*"
    ],
    "files": [
        "./src/button.ts",
        "./src/index.ts",
        "./src/row.ts",
        "./src/rows.ts",
        "./src/textbox.ts",
        "./node_modules/bobril/index.d.ts",
        "./node_modules/bobril/index.js",
        "./node_modules/bobril/index.ts",
        "./node_modules/bobril/package.json",
        "./node_modules/bobril/tsconfig.json",
        "./node_modules/fun-model/dist/src/actionFactory.d.ts",
        "./node_modules/fun-model/dist/src/actionFactory.js",
        "./node_modules/fun-model/dist/src/actionFactory.ts",
        "./node_modules/fun-model/dist/src/debug.d.ts",
        "./node_modules/fun-model/dist/src/debug.js",
        "./node_modules/fun-model/dist/src/debug.ts",
        "./node_modules/fun-model/dist/src/helpers.d.ts",
        "./node_modules/fun-model/dist/src/helpers.js",
        "./node_modules/fun-model/dist/src/helpers.ts",
        "./node_modules/fun-model/dist/src/index.d.ts",
        "./node_modules/fun-model/dist/src/index.js",
        "./node_modules/fun-model/dist/src/index.ts",
        "./node_modules/fun-model/dist/src/store.d.ts",
        "./node_modules/fun-model/dist/src/store.js",
        "./node_modules/fun-model/dist/src/store.ts",
        "./node_modules/fun-model/package.json",
        "./node_modules/fun-model/README.md"
    ]
}
